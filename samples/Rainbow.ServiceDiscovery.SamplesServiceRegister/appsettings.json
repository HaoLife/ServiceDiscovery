{
  "Service": {
    "Application": {
      "name": "service",
      "url": "http://localhost:63052"
    },
    "Zookeeper": {
      "Connection": "127.0.0.1:2181",
      "SessionTimeout": "00:00:30",
      "IsRegister": true
    },
    "consul": {
      "Address": "http://127.0.0.1:8500",
      "IsRegister": true,
      "CheckPath": "health"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Rainbow": "Debug"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "Rainbow": "Debug"
      }
    }
  }
}
